    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.2",
    "font-awesome": "^4.6.3",
    "lodash": "^4.16.2",
    "md5": "^2.2.1",
    "ng2-bootstrap": "^1.1.5",
    "ng2-select": "^1.1.0",
    "papaparse": "^4.1.2",
    "path": "^0.12.7",
    "rxjs": "5.4.2",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.2",
    "@angular/compiler-cli": "^4.2.4",

{
  "name": "wl-hashfile-generator",
  "version": "0.3.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build-electron": "tsc --outDir dist src/electron/main.ts && cp src/electron/package.json dist",
    "build": "ng build && npm run build-electron",
    "electron": "electron dist/main.js",
    "electron-build": "npm run build && build",
    "package": "electron-packager dist --out=packages --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.0.0",
    "@angular/compiler": "~2.0.0",
    "@angular/core": "~2.0.0",
    "@angular/forms": "~2.0.0",
    "@angular/http": "~2.0.0",
    "@angular/platform-browser": "~2.0.0",
    "@angular/platform-browser-dynamic": "~2.0.0",
    "@angular/router": "~3.0.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.2",
    "font-awesome": "^4.6.3",
    "lodash": "^4.16.2",
    "md5": "^2.2.1",
    "ng2-bootstrap": "^1.1.5",
    "ng2-select": "^1.1.0",
    "papaparse": "^4.1.2",
    "path": "^0.12.7",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/electron": "^1.4.24",
    "@types/jasmine": "^2.2.30",
    "@types/lodash": "^4.14.36",
    "@types/node": "^6.0.45",
    "angular-cli": "^1.0.0-beta.17",
    "codelyzer": "~0.0.26",
    "electron": "^1.4.3",
    "electron-builder": "^7.14.1",
    "electron-packager": "^8.1.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "2.0.2"
  },
  "directories": {
    "output": "packages",
    "app": "dist",
    "buildResources": "build"
  },
  "build": {
    "appId": "com.wellandlighthouse.wl-hashfile-generator",
    "dmg": {},
    "win": {}
  }
}
